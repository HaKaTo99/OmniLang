{
  "scopeName": "source.omni",
  "name": "OmniLang",
  "patterns": [
    {
      "name": "keyword.control.omni",
      "match": "\\b(POLICY|RULE|ACTION|ALLOW|DENY|CHECK|IF|ELSE|FOR|WHILE|IN|MATCH|WITH|ACTOR|TRIGGER)\\b"
    },
    {
      "name": "constant.language.boolean.omni",
      "match": "\\b(true|false)\\b"
    },
    {
      "name": "constant.numeric.omni",
      "match": "-?\\b[0-9]+(\\.[0-9]+)?\\b"
    },
    {
      "name": "string.quoted.double.omni",
      "begin": "\"",
      "end": "\"",
      "patterns": [{ "name": "constant.character.escape.omni", "match": "\\\\." }]
    },
    {
      "name": "comment.line.double-slash.omni",
      "match": "//.*$"
    }
  ],
